    "@metamask/onboarding": "^1.0.1",
    "@openzeppelin/contracts": "^4.5.0",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "classnames": "^2.3.1",
    "next": "12.0.8",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "swr": "^1.2.1"
